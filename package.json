{
  "name": "chooks-template",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "wasmcc -x c contracts/index.c -o build/index.wasm -O2 -Wl,--allow-undefined && hook-cleaner-v3 build/index.wasm build/index.wasm",
    "test": "LOG_LEVEL=error VITE_CJS_TRACE=true vitest",
    "xrpld:start": "xrpld-netgen up:standalone --protocol xahau --version 2025.5.8-HEAD+1777",
    "xrpld:stop": "xrpld-netgen down:standalone --protocol xahau --version 2025.5.8-HEAD+1777",
    "trace": "docker logs --tail 100 -f xahau 2>&1 | cut -d ' ' -f 5- | grep -a -E 'Publishing ledger|HookInfo|HookTrace|HookError|HookEmit|preclaim result'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@transia/hooks-toolkit": "npm:@tequ/hooks-toolkit@2.0.0-beta.1",
    "@xahau/hooks-cli": "^2.0.3",
    "dotenv": "^16.4.1",
    "xahau": "npm:@tequ/xahau@4.0.0-beta.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@tsconfig/node18": "^18.2.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
